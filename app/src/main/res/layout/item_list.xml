        android:padding="5dp" />

    <TextView
        android:id="@+id/textView_list_Soil_mos"
        android:layout_width="35dp"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:padding="5dp" />

    <TextView
        android:id="@+id/textView_list_Light"
        android:layout_width="35dp"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:padding="5dp" />

    <TextView
        android:id="@+id/textView_list_UploadTime"
        android:layout_width="173dp"
